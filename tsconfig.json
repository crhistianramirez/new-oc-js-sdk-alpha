{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "./dist",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist",
    "target": "es5"
  },
  "include": [
    "src/**/*"
  ],
  "typedocOptions": {
    "mode": "file",
    "out": "docs/api-reference",
    "name": "ordercloud-javascript-sdk",
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "theme": "docs/api-reference-templates"
  }
}